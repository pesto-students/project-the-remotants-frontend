{
  "presets": ["env", "es2015", "react"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    [
      "transform-runtime",
      {
        "regenerator": true
      }
    ],
    [
      "import", {
        "libraryName": "antd",
        "style": true
      }
    ]
  ]
}